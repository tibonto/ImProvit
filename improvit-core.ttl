@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix improvit: <http://www.project-improvit.de/vocab/> .

<https://github.com/tibonto/ImProvit/blob/master/improvit-core.ttl> rdf:type owl:Ontology.


#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype ;
         rdfs:label "date"@en .

###  http://www.project-improvit.de/vocab/Person
improvit:Person rdf:type owl:Class ;
                <http://purl.org/dc/elements/1.1/date> "2022-07-27T00:00:00"@en ;
                rdfs:comment "This class represents a person who is a human being."@en ;
                rdfs:isDefinedBy "Hanan, Shehneela, Janyn, Shahi, Annett."@en ;
                rdfs:label "Person"@en ;
                owl:versionInfo "v1.0"@en .

###  http://www.project-improvit.de/vocab/Donor
improvit:Donor rdf:type owl:Class ;
               rdfs:subClassOf improvit:Person ;
               <http://purl.org/dc/elements/1.1/date> "2022-07-27T00:00:00"@en ;
               rdfs:comment "This class represents a donor who participates in the study and donates biosamples."@en ;
               rdfs:isDefinedBy "Hanan, Shehneela, Janyn, Shahi, Annett."@en ;
               rdfs:label "Donor"@en ;
               owl:versionInfo "v1.0"@en .


###  http://www.project-improvit.de/vocab/SufferClass
improvit:SufferClass rdf:type owl:Class ;
                    <http://purl.org/dc/elements/1.1/date> "2023-09-20T00:00:00"@en ;
                    rdfs:comment "This class represents the relationship between donors and their disorders."@en ;
                    rdfs:isDefinedBy "Shahi"@en ;
                    rdfs:label "SufferClass"@en ;
                    owl:versionInfo "v1.0"^^xsd:string .

###  http://www.project-improvit.de/vocab/sufferingPerson
improvit:sufferingPerson rdf:type owl:ObjectProperty ;
                         rdfs:domain improvit:SufferClass ;
                         rdfs:range improvit:Donor ;
                         <http://purl.org/dc/elements/1.1/date> "2022-08-01T00:00:00"@en ;
                         rdfs:comment "This property shows the association of donor to suffer."@en ;
                         rdfs:isDefinedBy "Shehneela, Shahi, Annett, Hanan, Janyn."@en ;
                         rdfs:label "sufferingPerson"@en ;
                         owl:versionInfo "v1.0"@en .
